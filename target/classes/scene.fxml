<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.vse.seso00.MainController">
    <top>
        <VBox BorderPane.alignment="CENTER">
            <Label fx:id="locationName"/>
            <Label fx:id="locationDescription"/>
        </VBox>
    </top>
    <left></left>
    <right>
        <VBox spacing="20" BorderPane.alignment="CENTER">
            <Label>
                <font>
                    <Font size="15.0"/>
                </font>
            </Label>

            <VBox spacing="20">
                <Label>
                    <text>Východy</text>
                    <font>
                        <Font size="15.0"/>
                    </font>
                </Label>
                <VBox fx:id="exits"></VBox>

            </VBox>
            <VBox>
                <Label>
                    <text>Předměty</text>
                    <font>
                        <Font size="15.0"/>
                    </font>
                </Label>
                <VBox fx:id="items"></VBox>

            </VBox>

        </VBox>

    </right>
    <bottom>
        <VBox>
        <TextArea fx:id="textOutput" editable="false"></TextArea>
        <TextField fx:id="textInput" onKeyPressed="#onInputKeyPressed"></TextField>
        </VBox>
    </bottom>


</BorderPane>
